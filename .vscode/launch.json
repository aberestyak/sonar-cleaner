  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}/cmd/sonar-cleaner",
      "env": {
        "SONARQUBE_ADDRESS": "https://sonarqube.example.com",
        "SONARQUBE_TOKEN": "<PUT TOKEN HERE>",
        "KEEP_DAYS": "150",
        // "SONAR_CLEANER_DRY_RUN": "true",
        "SONAR_CLEANER_LOG_LEVEL": "debug"
      }
    }
  ]
}
